#test s21_string_strtok_test
char str_1[50] = "Hello, world!";
char str_2[50] = "Hello, world!";
char str_3[25] = "school 21";
char str_4[25] = "school 21";
char str_5[20] = "banana";
char str_6[20] = "banana";
char str_7[] = " \n\t";
char str_8[] = " \n\t";
char str_9[20] = "";
char str_10[20] = "342";

ck_assert_pstr_eq(strtok(str_1, " ,!"), s21_strtok(str_2, " ,!"));
ck_assert_pstr_eq(strtok(str_3, " "), s21_strtok(str_4, " "));
ck_assert_pstr_eq(strtok(str_5, "a"), s21_strtok(str_6, "a"));
ck_assert_pstr_eq(strtok(str_7, " \n\t"), s21_strtok(str_8, " \n\t"));
ck_assert_pstr_eq(strtok(str_9, "."), s21_strtok(str_9, "."));
ck_assert_pstr_eq(strtok(str_10, "."), s21_strtok(str_10, "."));
ck_assert_pstr_eq(strtok(str_10, "."), s21_strtok(str_10, "."));

