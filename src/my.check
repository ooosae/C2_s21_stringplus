    ck_assert_int_eq(strncmp(strs[i], strs[i], i), s21_strncmp(strs[i], strs[i], i));
}

#test s21_string_memchr_test
char str_1[] = "123456789";
char str_2[] = "";
char str_3[] = "sf f\nwweeeeeeeeeeeeee";
char str_4[30] = " \t\r\0";
char str_5[] = " \n\0\0";
char str_6[] = "Hello wrld\0";
char str_7[] = "\0"; 
ck_assert_pstr_eq(memchr(str_1, '9', 9), s21_memchr(str_1, '9', 9));
ck_assert_pstr_eq(memchr(str_2, '0', 3), s21_memchr(str_2, '0', 3));
ck_assert_pstr_eq(memchr(str_3, '\0', 3), s21_memchr(str_3, '\0', 3));
ck_assert_pstr_eq(memchr(str_4, 't', 16), s21_memchr(str_4, 't', 16));
ck_assert_pstr_eq(memchr(str_5, ' ', 4), s21_memchr(str_5, ' ', 4));
ck_assert_pstr_eq(memchr(str_6, 'i', 30), s21_memchr(str_6, 'i', 30));
ck_assert_pstr_eq(memchr(str_7, 'b', 0), s21_memchr(str_7, 'b', 0));

#test s21_string_memcpy_test
char str_1[20] = "tiew e r a";
char str_2[20] = " \0";
char str_3[20] = "tiew e r a";
char str_4[12] = "tiew\ne r ar";
char str_5[15] = " sdfswqrwr\0";
char str_6[12] = "tiew\ne r ar";
char str_7[50] = " \n\0\0";
char str_8[50] = " \n\0\0";
ck_assert_str_eq(memcpy(str_1, str_2, 8), s21_memcpy(str_3, str_2, 8));
ck_assert_str_eq(memcpy(str_1, str_2, 2), s21_memcpy(str_3, str_2, 2));
ck_assert_str_eq(memcpy(str_4, str_5, 9), s21_memcpy(str_6, str_5, 9));
ck_assert_str_eq(memcpy(str_4, str_5, 3), s21_memcpy(str_6, str_5, 3));
ck_assert_str_eq(memcpy(str_7, str_4, 0), s21_memcpy(str_8, str_4, 0));
ck_assert_str_eq(memcpy(str_7, str_5, 2), s21_memcpy(str_8, str_7, 2));
ck_assert_pstr_eq(memcpy(str_5, str_5, 0), s21_memcpy(str_5, str_5, 0));
ck_assert_pstr_eq(memcpy(str_5, str_5, 10), s21_memcpy(str_5, str_5, 10));
  
#test s21_string_memmove_test
char str_1[20] = "abcde";
char str_2[20] = "123\0";
char str_3[20] = "abcde";
char str_4[12] = "xyz";
char str_5[10] = "school21\0";
char str_6[12] = "xyz";
char str_7[50] = "longer string\0";
char str_8[50] = "longer string\0";
memmove(str_1, str_2, 4);
s21_memmove(str_3, str_2, 4);
ck_assert_str_eq(str_1, str_3);

memmove(str_1, str_2, 2);
s21_memmove(str_3, str_2, 2);
ck_assert_str_eq(str_1, str_3);

memmove(str_4, str_5, 7);
s21_memmove(str_6, str_5, 7);
ck_assert_str_eq(str_4, str_6);

memmove(str_4, str_5, 3);
s21_memmove(str_6, str_5, 3);
ck_assert_str_eq(str_4, str_6);

memmove(str_7, str_4, 0);
s21_memmove(str_8, str_4, 0);
ck_assert_str_eq(str_7, str_8);

memmove(str_7, str_5, 3);
s21_memmove(str_8, str_5, 3);
ck_assert_str_eq(str_7, str_8);

memmove(str_5, str_5, 4);
s21_memmove(str_5, str_5, 4);
ck_assert_str_eq(str_5, str_5);

